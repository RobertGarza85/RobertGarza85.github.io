---
import Project from "@/components/molecules/Project.astro";
import { projects as p } from "@/content/data/projects.json";
---

{
  p.map((project) => (
    <>
      <Project
        name={project.name}
        description={project.description}
        description2={project.description2}
        description3={project.description3}
        href={project.link}
        ctext={project.custom?.text}
        clink={project.custom?.link}
        langs={project.langs}
        tools={project.tools}
        other={project.other}
        date={project.date}
        images={project.images}
      />
      <br />
      <br />
    </>
  ))
}
